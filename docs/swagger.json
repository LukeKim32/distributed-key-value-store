{
    "swagger": "2.0",
    "info": {
        "description": "This service presents group of Redis nodes and the interface of them\nto test master/slave replication, data persistence, failover redemption",
        "title": "Redis-Cluster/Sentinel-Like Server",
        "contact": {
            "name": "김예찬 (Luke Kim)",
            "email": "burgund32@gmail.com"
        },
        "license": {},
        "version": "1.1"
    },
    "host": "localhost:8888",
    "basePath": "/api/v1",
    "paths": {
        "/clients": {
            "get": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Get Currently Registered Master/Slave Redis Clients",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/response.RedisListTemplate"
                        }
                    },
                    "500": {
                        "description": "서버 오류",
                        "schema": {
                            "$ref": "#/definitions/response.BasicTemplate"
                        }
                    }
                }
            },
            "post": {
                "description": "**Slave 추가 시,** 반드시 요청 바디에 **\"master_address\" 필드에 타겟 노드 주소 설정**\nMaster, Slave 운용하고 싶지 않은 경우, 모두 Master로 등록",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Add New Master/Slave Redis Clients",
                "parameters": [
                    {
                        "description": "Specifying Role and Address of New Node",
                        "name": "newSetData",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.NewClientRequestContainer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/response.RedisListTemplate"
                        }
                    },
                    "500": {
                        "description": "서버 오류",
                        "schema": {
                            "$ref": "#/definitions/response.BasicTemplate"
                        }
                    }
                }
            }
        },
        "/hash/data": {
            "post": {
                "description": "## Key, Value 쌍 저장\n**기존 값이 존재할 경우 덮어씌워진다**",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Set new Key, Value Pair",
                "parameters": [
                    {
                        "description": "Multiple Pairs can be set",
                        "name": "newSetData",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.DataRequestContainer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/response.SetResultTemplate"
                        }
                    },
                    "500": {
                        "description": "서버 오류",
                        "schema": {
                            "$ref": "#/definitions/response.BasicTemplate"
                        }
                    }
                }
            }
        },
        "/hash/data/{key}": {
            "get": {
                "description": "## 요청한 Key 값에 저장된 Value 값 가져오기",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Get stored Value with passed Key",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Target Key",
                        "name": "key",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/response.GetResultTemplate"
                        }
                    },
                    "500": {
                        "description": "서버 오류",
                        "schema": {
                            "$ref": "#/definitions/response.BasicTemplate"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "cluster.KeyValuePair": {
            "type": "object",
            "properties": {
                "key": {
                    "type": "string"
                },
                "value": {
                    "type": "string"
                }
            }
        },
        "cluster.RedisClient": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string"
                },
                "role": {
                    "type": "string"
                }
            }
        },
        "models.DataRequestContainer": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/cluster.KeyValuePair"
                    }
                }
            }
        },
        "models.NewClientRequestContainer": {
            "type": "object",
            "properties": {
                "address": {
                    "description": "Address : 레디스 노드 주소, IP + Port",
                    "type": "string"
                },
                "master_address": {
                    "description": "MasterAddress : Role = \"Slave\" 일 때 반드시 필요한 옵션 (Role = \"Master\" 일 경우 무시)",
                    "type": "string"
                },
                "role": {
                    "description": "Role : \"Master\" / \"Slave\"",
                    "type": "string"
                }
            }
        },
        "response.BasicTemplate": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                },
                "next_link": {
                    "type": "object",
                    "$ref": "#/definitions/response.NextLink"
                }
            }
        },
        "response.GetResultTemplate": {
            "type": "object",
            "properties": {
                "handled_node": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                },
                "next_link": {
                    "type": "object",
                    "$ref": "#/definitions/response.NextLink"
                },
                "result": {
                    "type": "string"
                }
            }
        },
        "response.NextLink": {
            "type": "object",
            "properties": {
                "href": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        },
        "response.RedisListTemplate": {
            "type": "object",
            "properties": {
                "masters": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/cluster.RedisClient"
                    }
                },
                "message": {
                    "type": "string"
                },
                "next_link": {
                    "type": "object",
                    "$ref": "#/definitions/response.NextLink"
                },
                "slaves": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/cluster.RedisClient"
                    }
                }
            }
        },
        "response.RedisResult": {
            "type": "object",
            "properties": {
                "handled_node": {
                    "type": "string"
                },
                "result": {
                    "type": "string"
                }
            }
        },
        "response.SetResultTemplate": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                },
                "next_link": {
                    "type": "object",
                    "$ref": "#/definitions/response.NextLink"
                },
                "results": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/response.RedisResult"
                    }
                }
            }
        }
    }
}